<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.jrue.poc.springmybatis.persistence.UserRoleMapper">

	<cache />

	<select id="findRolesByUserId" parameterType="int" resultType="UserRole">
		SELECT 
			*
		FROM
			M_USERROLE
		WHERE USERID = #{value}
		ORDER BY ID
	</select>

</mapper>